<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="285dp"
    android:orientation="vertical"
    android:padding="5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/NewsFeed_CustomList_Emblem"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:src="@drawable/ball" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">



                <TextView
                    android:id="@+id/NewsFeed_CustomList_Data"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Medium Text"
                    android:textSize="20dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/NewsFeed_CustomList_Court"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="박효근"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/NewsFeed_CustomList_Person"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Medium Text"
                    android:layout_weight="1"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/NewsFeed_CustomList_Time"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Medium Text"
                    android:textSize="20dp" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
    <ImageView
        android:id="@+id/NewSpeed_ImageView"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="#000000"
        android:scaleType="fitXY"
        android:visibility="gone" />
</LinearLayout>
